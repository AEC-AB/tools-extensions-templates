    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Python",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "env": {
                "PYTHONPATH": "${env:LOCALAPPDATA}\\Tools by AEC\\apps\\Assistant\\Resources\\PythonModules\\Assistant"
            }
        },
        {
            "name": "Attach Process Id",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Debug Assistant",
            "type": "coreclr",
            "request": "attach",
            "processName": "CW.Assistant.Viewer.exe"
        }
    ]
}